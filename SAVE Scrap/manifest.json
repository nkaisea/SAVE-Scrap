{
  "manifest_version": 2,
  "name": "SAVE Scrapbox",
  "short_name": "SaveScrap",
  "description": "Scrapboxの表示ページをtxtでダウンロードする",
  "version": "1.0.0.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["tabs", "https://scrapbox.io/*"],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
